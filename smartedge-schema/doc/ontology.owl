        <rdfs:comment>Unique identifier of a swarm node.</rdfs:comment>
        <rdfs:label>node identifier</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/smartedge/smartedge-schema#NodeProtocol -->


    <owl:Class rdf:about="https://w3id.org/smartedge/smartedge-schema#NodeProtocol">
